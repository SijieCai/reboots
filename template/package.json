{
  "name": "<%=projectName%>",
  "private": true,
  "version": "0.0.1",
  "description": "<%=projectName%>",
  "main": "",
  "scripts": {
    "start": "npm run serve:dev",
    "serve:dev": "webpack-dev-server --config=webpack.config.js --content-base src/ --inline",
    "serve:dist": "webpack-dev-server --env dist --config=webpack.config.js --content-base src/ --inline",
    "copy": "node -e \"require('directory-copy')({src: __dirname + '/src',dest: __dirname + '/dist',excludes: [/^\\app/]},function(err){console.log('done!')});\"",
    "clean": "rimraf src/build/* dist/*",
    "release:dev": "npm run clean && webpack --config=webpack.config.js && npm run copy",
    "release:dist": "npm run clean && webpack --env dist --config=webpack.config.js && npm run copy"
  },
  "repository": "",
  "keywords": [
    "<%=projectName%>"
  ],
  "author": "Your name here",
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-eslint": "^4.1.8",
    "css-loader": "^0.23.0",
    "directory-copy": "^0.1.0",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.11.3",
    "file-loader": "^0.8.4",
    "glob": "^6.0.0",
    "lodash": "^3.10.1",
    "minimist": "^1.2.0",
    "postcss-loader": "^0.8.0",
    "rimraf": "^2.4.3",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "history": "^2.1.1",
    "react": "^15.1.0",
    "react-bootstrap": "^0.29.4",
    "react-dom": "^15.1.0",
    "react-router": "^2.4.1"
  }
}
